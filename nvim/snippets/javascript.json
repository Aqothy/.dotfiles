{
  "console.log": {
    "prefix": "l",
    "body": "console.log(${0})"
  },
  "try/catch": {
    "prefix": "tc",
    "body": "try {\n\t${0}\n} catch (${1:err}) {\n\t\n}"
  },
  "function": {
    "prefix": "fn",
    "body": "function ${1:name}(${2:params}) {\n\t${0}\n}"
  },
  "async function": {
    "prefix": "afn",
    "body": "async function ${1:name}(${2:params}) {\n\t${0}\n}"
  }
}
