{
  "agent": {
    "default_profile": "minimal",
    "default_model": {
      "provider": "google",
      "model": "gemini-2.5-pro"
    }
  },
  "base_keymap": "VSCode",
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "vim_mode": true,
  "ui_font_size": 15,
  "buffer_font_size": 15,
  "theme": {
    "mode": "system",
    "light": "Gruvbox Light Soft",
    "dark": "Gruvbox Dark Soft"
  }
}
