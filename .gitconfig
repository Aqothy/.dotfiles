# This is Git's per-user configuration file.
[user]
	email = aq0335@gmail.com
	name = Aqothy
# Please adapt and uncomment the following lines:
#	name = Anthony Qiu
#	email = aq0335@gmail.com

[column]
    ui = auto

[branch]
    sort = -committerdate

[credential]

[core]
	excludesfile = ~/.gitignore

[alias]
	tree = log --graph --oneline --decorate --all

[push]
	autoSetupRemote = true

[pull]
    rebase = true

[merge]
	conflictstyle = diff3

[help]
    autocorrect = prompt

[diff]
    algorithm = histogram
    renames = true
